{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": ["es6"],
    "outDir": "build",
    "baseUrl": ".",
    "paths": {
      "*": [
          "node_modules/*",
          "src/types/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
